        <section class="content-main">
            <div class="content-header">
                <h3 class="content-title">Manage Creators </h3>   
                <!-- <div>
                    <a href="#" class="btn btn-primary">Create new</a>
                </div> -->
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <!-- <h4>Edit</h4> -->

                    <div class="col-md-12">
                        <table class="table table-bordered table-lg">
                            <thead>
                                <tr>
                                    <th class="fs-5" style="width: 20%;"><h6>User Id</h6></th>
                                    <th class="fs-5" style="width: 20%;"><h6>Name</h6></th>
                                    <th class="fs-5" style="width: 20%;"><h6>email</h6></th>
                                    <th class="fs-5" style="width: 20%;"><h6>phoneNumber</h6></th>
                                    <th class="fs-5" style="width: 20%;"><h6>Status</h6></th>
                                    <th class="fs-5 text-center" style="width: 20%;"><h5>Actions</h5></th>
                                </tr>
                            </thead>
                            <tbody>  
                                <% creatorcollection.reverse().forEach(creator => { %>  
                                    <tr>
                                        <td style="width: 20%;"><%= creator._id %></td>
                                        <td style="width: 10%;"><%= creator.creatorName %></td>
                                        <td style="width: 15%;"><%= creator.email %></td>
                                        <td style="width: 15%;"><%= creator.phone %></td>
                                        <td style="width: 15%;"><%= creator.status %></td>
                                        <td style="width: 25%;">
                                            <% if(creator.status === 'unblocked') { %>
                                                <a href="/admin/block-creator/<%= creator._id %>" class="btn btn-primary">Block</a>  
                                            <% } else if (creator.status === 'Block') {%> 
                                                <a href="/admin/unblock-creator/<%= creator._id %>" class="btn btn-danger">Unblock</a>
                                            <%} %>
                                            
                                        </td>
                                    </tr> 
                                <% }) %>
                            </tbody>
                        </table>
                    </div>

